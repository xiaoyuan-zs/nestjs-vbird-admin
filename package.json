	"devDependencies": {
		"@eslint/js": "^9.10.0",
		"@nestjs/cli": "^10.4.5",
		"@nestjs/mapped-types": "^2.0.5",
		"@nestjs/schematics": "^10.1.4",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"globals": "^15.9.0",
		"husky": "^9.1.5",
		"lint-staged": "^15.2.10",
